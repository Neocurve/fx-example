//--------------------------------
	
	CALL FC20 (
	   EM_ID                    := EM_I_OUT_ID,
	   UDT_LOCATION             := #EM_I_OUT,
	   UDT_OFFSET               := 40,
	   Length                   := 40,
	   EM_addressing_error      := M      0.0,
	   RETVAL                   := MW     0);
	
	CALL FC20 (
	   EM_ID                    := Some_Dyn_EM_ID,
	   UDT_LOCATION             := #Some_Dyn_EM,
	   UDT_OFFSET               := 40,
	   Length                   := 40,
